<%= form_for @spectrum_range, html: {class: "form-horizontal"} do |f| %>
  <% if @spectrum_range.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@spectrum_range.errors.count, "error") %> prohibited this spectrum_range from being saved:</h2>

      <ul>
      <% @spectrum_range.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field control-group">
    <%= f.label :name, class: "control-label" %><br />
    <%= f.text_field :name, class: "controls" %>
  </div>
  <div class="field">
		<%= f.label :low, 'Lower End of Spectrum Band' %><br />
    <%= f.number_field :low %>
  </div>
  <div class="field">
    <%= f.label :high, 'Upper End of Spectrum Band' %><br />
    <%= f.number_field :high %>
  </div>
  <div class="field">
    <%= f.label :issue_id %><br />
    <%= f.number_field :issue_id %>
  </div>
 <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
